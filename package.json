{
  "name": "supertracker",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=16.0.0"
  },
  "packageManager": "pnpm@7.22.0",
  "workspaces": [
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "cross-env FORCE_COLOR=1 turbo run dev --parallel",
    "test": "turbo run test --parallel",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\""
  },
  "devDependencies": {
    "@dev/eslint-config-superdao": "0.0.1",
    "@dev/tsconfig": "0.0.2",
    "cross-env": "7.0.3",
    "eslint": "8.36.0",
    "npm-check-updates": "16.8.0",
    "prettier": "2.8.7",
    "turbo": "1.8.6",
    "typescript": "4.9.5"
  }
}